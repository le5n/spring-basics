<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="configurableMessageProvider"
      class="com.le5n.constructor.ConfigurableMessageProvider">
    <constructor-arg value="Configurable message"/>
    <!--if there are constructors with same amount of values, you can also set the type of argument-->
</bean>
</beans>